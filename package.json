    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "passport": "^0.4.0",
    "passport-auth0": "^0.6.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-parallax": "^1.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^5.0.0",
    "stripe": "^5.5.0"
  },
  "scripts": {
